pipeline{
	agent any
	stages {
		stage('Hello') {
			steps {
				echo 'hello world'
			}
		}
		stage('Build') {
			steps {
				echo 'hello build'
			}
		}
		stage('Deploy') {
			steps {
				echo 'hello deploy'
			}
		}
		stage('Test') {
			steps {
				echo 'hello Test'
			}
		}
		stage('Release') {
			steps {
				echo 'hello release'
			}
		}
	}
}
